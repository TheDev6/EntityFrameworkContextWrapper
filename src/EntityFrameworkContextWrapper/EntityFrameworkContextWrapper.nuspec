<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
    <metadata>
        <id>EntityFrameworkContextWrapper</id>
        <version>1.1.0</version>
        <title>Entity Framework Context Wrapper</title>
        <authors>TheDev6</authors>
        <projectUrl>https://github.com/TheDev6/EntityFrameworkContextWrapper</projectUrl>
        <iconUrl>https://dev6.blob.core.windows.net/blog-images/db-wrapper.jpg</iconUrl>
